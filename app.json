{
  "expo": {
    "name": "rn-expo",
    "slug": "rn-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/EA/globo-de-plasma-300.png",
    "splash": {
      "image": "./assets/images/EA/EA-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#044a84"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/EA/globo-de-plasma-300.png",
        "backgroundColor": "#044a84"
      },
      "package": "com.dfellon.rnexpo"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/GodOfThunder.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "baseUrl": "/rn-expo",
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7b3c4ca-785a-43aa-a20b-49ac6e4620b8"
      }
    }
  }
}
