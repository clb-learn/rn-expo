{
  "expo": {
    "name": "rn-expo",
    "slug": "rn-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EA/globo de plasma - 7.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "repo": "github",
    "splash": {
      "image": "./assets/images/EA/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00559c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1b1d22"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
         "expo-font",
         {
            "fonts": ["./assets/fonts/GodOfThunder.ttf"]
         }
      ]
    ],
    "experiments": {
      "baseUrl": "/rn-expo",
      "typedRoutes": true
    }
  }
}
